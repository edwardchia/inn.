  <div class="card-body">
    <h5 class="card-title"> <%=listing.address%></h5>
    <p class="card-text">To see details of this listing press "Book Me" ğŸ‘‡</p>
    <%= link_to "Book Me", listing_path(listing.id), class: "btn btn-primary" %>

  </div>
</div>

</div>
  <% end %>
</div>
 
